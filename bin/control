#!/bin/bash

source $OPENSHIFT_CARTRIDGE_SDK_BASH


function start() {
 :
}

function stop() {
  :
}

function restart() {
    stop
    start
}

function status() {
  return 0
}

function reload() {
  :
}

function tidy() {
  echo "Nothing to clean for oracle."
}

case "$1" in
  start) start ;;
  stop) stop ;;
  restart) restart ;;
  status) status ;;
  reload) reload ;;
  tidy) tidy ;;
  *) exit 0
esac
